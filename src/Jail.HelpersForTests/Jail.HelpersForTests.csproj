<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461;net45</TargetFrameworks>
    <Version>1.0.1.0</Version>
    <Authors>George Minashin</Authors>
    <Company />
    <Description>Just Another Infrastructure Library. Facilitates unit (and possibly integration) testing.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/hoborg91/jail</PackageProjectUrl>
    <PackageIcon>Jail.HelpersForTests.png</PackageIcon>
    <PackageTags>infrastructure common testing unit</PackageTags>
    <Copyright>Copyright 2020</Copyright>
    <PackageReleaseNotes></PackageReleaseNotes>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>Jail.HelpersForTests.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\icons\Jail.HelpersForTests.png" Pack="true" PackagePath="\"/>
  </ItemGroup>

</Project>
